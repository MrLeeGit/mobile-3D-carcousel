{
  "name": "@mfelibs/base-utils",
  "version": "1.5.3",
  "description": "手浪前端基础库",
  "module": "src/index.js",
  "main": "lib/core.cjs.js",
  "homepage": "",
  "files": ["src/*", "README.md", "lib/*"],
  "scripts": {
    "test": "npm run lint",
    "serve": "browser-sync start --no-ui --server --files \"index.html, test/bundle.js\"",
    "dev": "rollup -w -c build/dev.config.js",
    "dev:page": "rollup -wc build/test.config.js & npm run serve",
    "build:zepto": "node build/build-zepto",
    "build:core": "node build --core",
    "build": "node build --prod",
    "lint": "eslint src",
    "release": "bash build/release.sh"
  },
  "keywords": ["mfelibs", "zepto", "utils"],
  "author": "Vincent Lee <wensen@staff.sina.com.cn>",
  "license": "MIT",
  "bugs": "http://gitlab/SINA_MFE_COMPONENTS/base-utils/issues",
  "repository": {
    "type": "git",
    "url": "git@gitlab:SINA_MFE_COMPONENTS/base-utils.git"
  },
  "dependencies": {},
  "devDependencies": {
    "@mfelibs/eslint-config-mfe": "^1.1.0",
    "babel": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.23.0",
    "eslint": "^3.17.1",
    "fastclick": "^1.0.6",
    "ora": "^1.1.0",
    "rollup": "^0.41.4",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^7.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-watch": "^3.2.2",
    "shelljs": "^0.7.7",
    "uglify-js": "^2.8.0"
  }
